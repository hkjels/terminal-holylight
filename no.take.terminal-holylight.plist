<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>


    <!-- Launching and behavior -->

    <key>Disabled</key>
    <false/>
    <key>Label</key>
    <string>no.take.terminal-holylight</string>
    <key>ProgramArguments</key>
    <array>
      <string>{{PREFIX}}/opt/terminal-holylight/terminal-holylight.app/Contents/MacOS/applet</string>
    </array>
    <key>RunAtLoad</key>
    <true/>


    <!-- Preferences -->

    <key>Light</key>
    <string>{{LIGHT}}</string>

    <key>Dark</key>
    <string>{{DARK}}</string>

    <key>Threshold</key>
    <integer>{{THRESHOLD}}</integer>

    <key>Throttle</key>
    <integer>{{THROTTLE}}</integer>


  </dict>
</plist>
